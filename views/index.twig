{% extends 'layout.twig' %}

{% block body %}
<div>
  <form action="/upload" method="post" encType="multipart/form-data">
    <h1>File upload server</h1>
    <input type="file" name="file"/>
    <input type="text" name="token" placeholder="Token">
    <button type="submit">Upload file</button>
  </form>
</div>
{% endblock %}
